	<modbus.modbusPort datatype="INT16" modbusfunction="3" modbuslen="1" modbusstartaddr="270" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_AUTOTUNE_RBV" slaveaddr="1" tcpportname="$(port)"/>
	<modbus.modbusPort datatype="INT16" modbusfunction="6" modbuslen="1" modbusstartaddr="270" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_AUTOTUNE" slaveaddr="1" tcpportname="$(port)"/>
	<modbus.modbusPort datatype="INT16" modbusfunction="3" modbuslen="1" modbusstartaddr="30" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_OUTPHI_RBV" slaveaddr="1" tcpportname="$(port)"/>
	<modbus.modbusPort datatype="INT16" modbusfunction="6" modbuslen="1" modbusstartaddr="30" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_OUTPHI" slaveaddr="1" tcpportname="$(port)"/>
