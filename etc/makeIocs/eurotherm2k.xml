	<modbus.modbusPort datatype="INT16" modbusfunction="6" modbuslen="1" modbusstartaddr="273" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_MAN" slaveaddr="1" tcpportname="$(port)"/>
	<modbus.modbusPort datatype="INT16" modbusfunction="3" modbuslen="1" modbusstartaddr="6" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_PROP_RBV" slaveaddr="1" tcpportname="$(port)"/>
	<modbus.modbusPort datatype="INT16" modbusfunction="6" modbuslen="1" modbusstartaddr="6" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_PROP" slaveaddr="1" tcpportname="$(port)"/>
	<modbus.modbusPort datatype="INT16" modbusfunction="3" modbuslen="1" modbusstartaddr="8" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_INTEG_RBV" slaveaddr="1" tcpportname="$(port)"/>
