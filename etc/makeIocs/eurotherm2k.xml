	<modbus.modbusPort datatype="INT16" modbusfunction="3" modbuslen="1" modbusstartaddr="9" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_DERIV_RBV" slaveaddr="1" tcpportname="$(port)"/>
	<modbus.modbusPort datatype="INT16" modbusfunction="6" modbuslen="1" modbusstartaddr="9" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_DERIV" slaveaddr="1" tcpportname="$(port)"/>
	<modbus.modbusPort datatype="INT16" modbusfunction="3" modbuslen="1" modbusstartaddr="35" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_RAMPRATE_RBV" slaveaddr="1" tcpportname="$(port)"/>
