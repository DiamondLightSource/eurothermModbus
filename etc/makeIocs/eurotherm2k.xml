	<modbus.modbusPort datatype="INT16" modbusfunction="3" modbuslen="1" modbusstartaddr="3" plctype="" pollmsec="1000" portname="$(modbus_prefix)_L1_OUTPUT_RBV" slaveaddr="1" tcpportname="$(port)"/>
