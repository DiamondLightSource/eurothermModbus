    <resize_behaviour>2</resize_behaviour>
    <scripts>
      <path checkConnect="true" pathString="EmbeddedJs" seoe="false" sfe="false">
        <scriptName>OPIFileScript</scriptName>
        <scriptText>importPackage(Packages.org.csstudio.opibuilder.scriptUtil);
var pv0 = PVUtil.getLong(pvs[0]);
if(pv0==0){
var macroInput = DataUtil.createMacrosInput(true);
macroInput.put("loop", "LOOP1");
macroInput.put("label", "Loop 1");
widget.setPropertyValue("macros", macroInput);
widget.setPropertyValue("opi_file","eurotherm3504-loop.opi");
}
else if(pv0==1){
var macroInput = DataUtil.createMacrosInput(true);
macroInput.put("loop", "LOOP2");
macroInput.put("label", "Loop 2");
widget.setPropertyValue("macros", macroInput);
widget.setPropertyValue("opi_file","eurotherm3504-loop.opi");
}
</scriptText>
        <pv trig="true">loc://$(DID)looptab&lt;VEnum&gt;(0, "LOOP1", "LOOP2")</pv>
      </path>
    </scripts>
    <width>360</width>
    <x>15</x>
    <y>70</y>
  </widget>
